<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TextBoxTextCaretBrush" Color="Black" />

    <SolidColorBrush x:Key="TextBoxDefaultBorderBrush" Color="#CDCDCD" />
    <SolidColorBrush x:Key="TextBoxDefaultBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TextBoxPointeroverBackground" Color="#F9F9F9" />
    <SolidColorBrush x:Key="TextBoxPressedBackground" Color="#E0E0E0" />
    <SolidColorBrush x:Key="TextBoxFocusBorderBrush" Color="#54A9FF" />
    <SolidColorBrush x:Key="TextBoxForeground" Color="#1A1A1A" />
    <SolidColorBrush x:Key="TextBoxInnerForeground" Opacity="0.62" Color="#1A1A1A" />

    <SolidColorBrush x:Key="TextBoxDisabledBackground" Color="Transparent" />
    <SolidColorBrush x:Key="TextBoxDisabledForeground" Color="#999999" />
    <SolidColorBrush x:Key="TextBoxDisabledBorderBrush" Color="#DFDFDF" />

    <SolidColorBrush x:Key="TextBoxBorderedDefaultBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TextBoxBorderedDefaultBorderBrush" Color="#CDCDCD" />
    <SolidColorBrush x:Key="TextBoxBorderedPointeroverBackground" Color="#F9F9F9" />
    <SolidColorBrush x:Key="TextBoxBorderedPointeroverBorderBrush" Color="#CDCDCD" />

    <SolidColorBrush x:Key="TextBoxSelectionBackground" Color="#0059D6" />
    <SolidColorBrush x:Key="TextBoxSelectionForeground" Color="White" />
</ResourceDictionary>