<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="TextBoxTextCaretBrush" Color="White" />

    <SolidColorBrush x:Key="TextBoxDefaultBorderBrush" Color="#707070" />
    <SolidColorBrush x:Key="TextBoxDefaultBackground" Color="#3D3D3D" />
    <SolidColorBrush x:Key="TextBoxPointeroverBackground" Opacity="0.16" Color="White" />
    <SolidColorBrush x:Key="TextBoxPressedBackground" Color="#292929" />
    <SolidColorBrush x:Key="TextBoxFocusBorderBrush" Color="#54A9FF" />
    <SolidColorBrush x:Key="TextBoxForeground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TextBoxInnerForeground" Color="#FFFFFF" />

    <SolidColorBrush x:Key="TextBoxDisabledBackground" Opacity="0.04" Color="#E6E8EA" />
    <SolidColorBrush x:Key="TextBoxDisabledForeground" Opacity="0.20" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TextBoxDisabledBorderBrush" Color="#505050" />

    <SolidColorBrush x:Key="TextBoxBorderedDefaultBackground" Color="#3D3D3D" />
    <SolidColorBrush x:Key="TextBoxBorderedDefaultBorderBrush" Color="#707070" />
    <SolidColorBrush x:Key="TextBoxBorderedPointeroverBackground" Opacity="0.16" Color="White" />
    <SolidColorBrush x:Key="TextBoxBorderedPointeroverBorderBrush" Color="#7FC1FF" />

    <SolidColorBrush x:Key="TextBoxSelectionBackground" Color="#0059D6" />
    <SolidColorBrush x:Key="TextBoxSelectionForeground" Color="White" />
</ResourceDictionary>